{
  "strict": {
    "text-font-family": "Raleway-Regular",
    "text-font-family-bold": "Raleway-Bold",
    "text-font-family-ebold": "Raleway-ExtraBold",

    "text-heading-4-font-size": 28,
    "text-heading-4-font-weight": "600",
    "text-heading-4-font-family": "$text-font-family-ebold",
    "text-heading-5-font-size": 24,
    "text-heading-5-font-weight": "600",
    "text-heading-5-font-family": "$text-font-family-bold",
    "text-heading-6-font-size": 20,
    "text-heading-6-font-weight": "600",
    "text-heading-6-font-family:": "$text-font-family-bold",
    "text-subtitle-1-font-size": 15,
    "text-subtitle-1-font-weight": "600",
    "text-subtitle-1-font-family": "$text-font-family-bold",
    "text-subtitle-2-font-size": 14,
    "text-subtitle-2-font-weight": "600",
    "text-subtitle-2-font-family": "$text-font-family-bold",
    "text-paragraph-1-font-size": 15,
    "text-paragraph-1-font-weight": "600",
    "text-paragraph-1-font-family": "$text-font-family",
    "text-paragraph-2-font-size": 13,
    "text-paragraph-2-font-weight": "400",
    "text-paragraph-2-font-family": "$text-font-family",
    "text-caption-1-font-size": 12,
    "text-caption-1-font-weight": "400",
    "text-caption-1-font-family": "$text-font-family",
    "text-caption-2-font-size": 9,
    "text-caption-2-font-weight": "600",
    "text-caption-2-font-family": "$text-font-family",
    "text-label-font-size": 12,
    "text-label-font-weight": "600",
    "text-label-font-family": "$text-font-family-bold"
  },
  "components": {
    "Input": {
      "meta": {
        "variantGroups": {
          "size": {
            "giant": {
              "default": false
            }
          }
        }
      },
      "appearances": {
        "default": {
          "variantGroups": {
            "size": {
              "giant": {
                "minHeight": "size-giant",
                "borderRadius": "border-radius",
                "borderWidth": "border-width",
                "paddingVertical": 13,
                "textFontSize": "text-subtitle-1-font-size",
                "textFontWeight": "normal",
                "textMarginHorizontal": 0
              }
            }
          }
        }
      }
    }
  }
}
